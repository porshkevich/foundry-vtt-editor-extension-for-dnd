<form>
    <div class="form-group {{#if (not selected)}}hidden{{/if}}">
        <label>{{localize "EEFDND.SelectedValue"}}</label>
        <div class="form-fields">
            <input type="text" name="old" value="{{selected}}" {{disabled}}>
        </div>
    </div>
    <div class="form-group">
        <label>{{localize "DND5E.Formula"}}</label>
        <div class="form-fields">
            <input type="text" name="formula">
            <label for="temp">{{localize "DND5E.Temp"}}</label>
            <input name="temp" type="checkbox" value="temp">
        </div>
    </div>
    <button type="button" name="save">{{localize "EEFDND.EnricherInsert"}}</button>
</form>
